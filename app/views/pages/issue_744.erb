<table class="table table-striped table-bordered table-condensed">
	<thead>
	<tr style="background-color: #999999;">
		<th>t('activerecord.attributes.account.export.number')</th>
		<th>t('activerecord.attributes.account.export.name')</th>
		<th class="text-right">t('activerecord.attributes.account.export.total')</th>
		<th class="text-right">t('activerecord.attributes.account.export.debit')</th>
		<th class="text-right">t('activerecord.attributes.account.export.credit')</th>
	</tr>
	</thead>
	<tbody>
	<% (1..50).each do |n| %>
		<% if n != 0 %>
			<tr>
				<td style="font-weight: bold;padding:2px;"><%= n %></td>
				<td style="font-weight: bold;padding:2px 2px 2px 5px;"><%= n %></td>
				<td style="padding: 2px;"></td>
				<td style="text-align: right; font-weight: bold;padding:2px;">n</td>
				<td style="text-align: right; font-weight: bold;padding:2px;">n</td>
			</tr>
			<% (1..3).each do |n| %>
				<tr>
					<td style="padding: 1px;"></td>
					<td style="padding: 1px 1px 1px 50px;">n</td>
					<td style="padding: 1px; text-align: right;">n</td>
					<td style="padding: 1px;"></td>
					<td style="padding: 1px;"></td>
				</tr>
			<% end %>
		<% end %>
	<% end %>
	<% if true %>
		<tr>
			<td style="padding: 1px;"></td>
			<td style="font-weight: bold;padding:2px 2px 2px 5px;">Résultat d'exploitation</td>
			<td style="padding: 1px;"></td>
			<td style="padding: 1px;"></td>
			<td style="font-weight: bold;padding: 1px; text-align: right;">n</td>
		</tr>
	<% else %>
		<tr>
			<td style="padding: 1px;"></td>
			<td style="font-weight: bold;padding:2px 2px 2px 5px;">Résultat d'exploitation</td>
			<td style="padding: 1px;"></td>
			<td style="font-weight: bold;padding: 1px; text-align: right;">n</td>
			<td style="padding: 1px;"></td>
		</tr>
	<% end %>
	</tbody>
	<tfoot>
	<tr style="background-color:#AAAAAA;">
		<td></td>
		<td style="font-weight: bold">SOUS-TOTAUX</td>
		<td></td>
		<td style="text-align: right; font-weight: bold">n</td>
		<td style="text-align: right; font-weight: bold">n</td>
	</tr>
	<tr style="background-color:#AAAAAA;">
		<td></td>
		<td style="font-weight: bold">SOLDE</td>
		<td></td>
		<td style="text-align: right; font-weight: bold">n</td>
		<td style="text-align: right; font-weight: bold">n</td>
	</tr>
	<tr style="background-color:#AAAAAA;">
		<td></td>
		<td style="font-weight: bold">TOTAUX</td>
		<td></td>
		<td style="text-align: right; font-weight: bold">n </td>
		<td style="text-align: right; font-weight: bold">n </td>
	</tr>
	</tfoot>
</table>
